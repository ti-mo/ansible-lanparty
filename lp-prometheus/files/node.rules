# Alert for any instance that have a median request latency >1s.
ALERT MemHigh
  IF node_memory_Active/1e9 >= 10
  FOR 15m
  LABELS { 
    severity = "warning",
  }
  ANNOTATIONS {
    summary = "Device {{ $labels.instance }} has high mem usage",
    description = "{{ $labels.instance }} has high mem usage: {{ $value }}",
  }